{
  "name": "@ainative/ai-kit",
  "version": "0.0.1",
  "description": "The Stripe for LLM Applications - Framework-agnostic SDK for building AI-powered applications",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
