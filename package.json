{
  "name": "@ainative/ai-kit",
  "version": "0.0.1",
  "description": "The Stripe for LLM Applications - Framework-agnostic SDK for building AI-powered applications",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules",
    "docs:api": "tsx scripts/generate-api-docs.ts",
    "docs:typedoc": "typedoc",
    "docs:validate": "vitest run scripts/__tests__/api-docs.test.ts",
    "docs": "pnpm run docs:api && pnpm run docs:typedoc"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^9.39.1",
    "glob": "^13.0.0",
    "jsdom": "^27.2.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "tsx": "^4.20.6",
    "turbo": "^1.11.0",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.3.0",
    "vitest": "^1.6.1"
  },
  "packageManager": "pnpm@8.12.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
