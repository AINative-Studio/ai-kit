{
  "name": "support-agent",
  "version": "0.0.1",
  "private": true,
  "description": "Customer Support Agent - Multi-agent system with ticket classification and knowledge base",
  "scripts": {
    "dev": "concurrently \"next dev -p 3003\" \"node src/server/index.js\"",
    "build": "next build && tsc -p tsconfig.server.json",
    "start": "concurrently \"next start -p 3003\" \"node dist/server/index.js\"",
    "server": "tsx src/server/index.ts",
    "test": "vitest run",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ainative/ai-kit": "workspace:*",
    "@examples/shared": "workspace:*",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "express": "^4.18.2",
    "zod": "^3.22.4",
    "sentiment": "^5.0.2",
    "recharts": "^2.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.0",
    "@types/express": "^4.17.21",
    "typescript": "^5.3.0",
    "vitest": "^1.6.1",
    "concurrently": "^8.2.2",
    "tsx": "^4.20.6"
  }
}
