{
  "name": "@ainative/ai-kit-vue",
  "version": "0.1.0-alpha.2",
  "description": "AI Kit - Vue 3 composables for building AI-powered applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ainative",
    "ai",
    "llm",
    "typescript",
    "ai-agents",
    "ai-primitives",
    "ainative-developer",
    "claude",
    "claude-code",
    "openai",
    "anthropic",
    "vue",
    "vue3",
    "composition-api",
    "composables",
    "ui",
    "components"
  ],
  "author": "AINative Studio",
  "license": "MIT",
  "homepage": "https://ainative.studio/ai-kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/AINative-Studio/ai-kit.git",
    "directory": "packages/vue"
  },
  "bugs": {
    "url": "https://github.com/AINative-Studio/ai-kit/issues"
  },
  "dependencies": {
    "@ainative/ai-kit-core": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vue/test-utils": "^2.4.0",
    "happy-dom": "^12.10.3",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "vue": "^3.4.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
