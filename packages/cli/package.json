{
  "name": "@ainative/ai-kit-cli",
  "version": "0.1.1-alpha.5",
  "description": "AI Kit - CLI tool for scaffolding and managing AI-powered applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "bin": {
    "aikit": "./dist/index.js",
    "ai-kit": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsx watch src/index.ts",
    "test": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "ainative",
    "ai",
    "llm",
    "typescript",
    "ai-agents",
    "ai-primitives",
    "ainative-developer",
    "cli",
    "scaffold",
    "generator",
    "template",
    "developer-tools",
    "code-generation",
    "claude",
    "claude-code",
    "openai",
    "anthropic"
  ],
  "author": "AINative Studio",
  "license": "MIT",
  "homepage": "https://www.ainative.studio/ai-kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/AINative-Studio/ai-kit.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/AINative-Studio/ai-kit/issues"
  },
  "dependencies": {
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "conf": "^12.0.0",
    "degit": "^2.8.4",
    "dotenv": "^16.3.1",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.12",
    "listr2": "^8.0.0",
    "node-fetch": "^3.3.2",
    "ora": "^7.0.1",
    "semver": "^7.5.4",
    "tar": "^6.2.0",
    "update-notifier": "^7.0.0",
    "validate-npm-package-name": "^5.0.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.5",
    "@types/semver": "^7.5.6",
    "@types/tar": "^6.1.11",
    "@types/update-notifier": "^6.0.8",
    "@types/validate-npm-package-name": "^4.0.2",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "memfs": "^4.6.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
