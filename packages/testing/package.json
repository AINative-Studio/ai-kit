{
  "name": "@ainative/ai-kit-testing",
  "version": "0.0.1",
  "description": "Comprehensive test utilities for AI Kit - mocks, fixtures, helpers, and custom matchers",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./mocks": {
      "types": "./dist/mocks/index.d.ts",
      "import": "./dist/mocks/index.mjs",
      "require": "./dist/mocks/index.js"
    },
    "./fixtures": {
      "types": "./dist/fixtures/index.d.ts",
      "import": "./dist/fixtures/index.mjs",
      "require": "./dist/fixtures/index.js"
    },
    "./helpers": {
      "types": "./dist/helpers/index.d.ts",
      "import": "./dist/helpers/index.mjs",
      "require": "./dist/helpers/index.js"
    },
    "./matchers": {
      "types": "./dist/matchers/index.d.ts",
      "import": "./dist/matchers/index.mjs",
      "require": "./dist/matchers/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "testing",
    "mocks",
    "fixtures",
    "matchers",
    "jest",
    "vitest",
    "ainative"
  ],
  "author": "AINative Studio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AINative-Studio/ai-kit.git",
    "directory": "packages/testing"
  },
  "dependencies": {
    "@ainative/ai-kit-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^1.6.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.6.1"
  },
  "peerDependencies": {
    "vitest": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
