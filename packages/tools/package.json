{
  "name": "@ainative/ai-kit-tools",
  "version": "0.1.0-alpha.2",
  "description": "AI Kit - Built-in tools for agents including web search, calculator, code interpreter, and more",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ainative",
    "ai",
    "llm",
    "typescript",
    "ai-agents",
    "ai-primitives",
    "ainative-developer",
    "claude",
    "claude-code",
    "openai",
    "anthropic",
    "tools",
    "utilities",
    "helpers",
    "mcp",
    "integrations"
  ],
  "author": "AINative Studio",
  "license": "MIT",
  "homepage": "https://ainative.studio/ai-kit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AINative-Studio/ai-kit.git",
    "directory": "packages/tools"
  },
  "bugs": {
    "url": "https://github.com/AINative-Studio/ai-kit/issues"
  },
  "dependencies": {
    "@ainative/ai-kit-core": "workspace:*",
    "isolated-vm": "^5.0.0",
    "mathjs": "^15.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/mathjs": "^9.4.2",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
