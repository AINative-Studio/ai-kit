{
  "name": "@ainative/ai-kit-tools",
  "version": "0.0.1",
  "description": "Built-in tools for AI Kit agents - web search, calculator, code interpreter",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "llm",
    "tools",
    "agents",
    "ainative"
  ],
  "author": "AINative Studio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AINative-Studio/ai-kit.git",
    "directory": "packages/tools"
  },
  "dependencies": {
    "@ainative/ai-kit-core": "workspace:*",
    "isolated-vm": "^6.0.2",
    "mathjs": "^15.1.0",
    "vm2": "^3.10.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/mathjs": "^9.4.2",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
