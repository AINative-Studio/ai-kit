{
  "name": "@ainative/ai-kit-nextjs",
  "version": "0.0.1",
  "description": "Next.js-specific utilities for AI Kit - middleware helpers, streaming, and more",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "import": "./dist/middleware/index.mjs",
      "require": "./dist/middleware/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "nextjs",
    "middleware",
    "streaming",
    "auth",
    "rate-limiting",
    "ainative"
  ],
  "author": "AINative Studio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AINative-Studio/ai-kit.git",
    "directory": "packages/nextjs"
  },
  "peerDependencies": {
    "next": ">=13.0.0"
  },
  "dependencies": {
    "@ainative/ai-kit-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^1.0.0",
    "next": "^14.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
