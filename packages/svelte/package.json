{
  "name": "@ainative/ai-kit-svelte",
  "version": "0.1.0-alpha.3",
  "description": "AI Kit - Svelte stores and actions for building AI-powered applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ainative",
    "ai",
    "llm",
    "typescript",
    "ai-agents",
    "ai-primitives",
    "ainative-developer",
    "claude",
    "claude-code",
    "openai",
    "anthropic",
    "svelte",
    "sveltekit",
    "framework",
    "reactive",
    "ui",
    "components"
  ],
  "author": "AINative Studio",
  "license": "MIT",
  "homepage": "https://github.com/AINative-Studio/ai-kit#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/AINative-Studio/ai-kit.git",
    "directory": "packages/svelte"
  },
  "bugs": {
    "url": "https://github.com/AINative-Studio/ai-kit/issues"
  },
  "dependencies": {
    "@ainative-studio/aikit-core": "^0.1.3"
  },
  "peerDependencies": {
    "svelte": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@testing-library/svelte": "^4.0.5",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^1.6.0",
    "jsdom": "^23.0.1",
    "svelte": "^4.2.8",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.6.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
